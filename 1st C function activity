#include <stdio.h>

int main()
{
    float marks1, marks2, marks3, marks4, marks5, average;

    printf("English: ");
    scanf("%f", &marks1);
    printf("Math :");
    scanf("%f", &marks2);
    printf("Science :");
    scanf("%f", &marks3);
    printf("PE: ");
    scanf("%f", &marks4);
    printf("Computer :");
    scanf("%f", &marks5);

    average = (marks1 + marks2 + marks3 + marks4 + marks5) / 3;
    printf("Average : %0.2f\n", average);

   
    return 0;
}
